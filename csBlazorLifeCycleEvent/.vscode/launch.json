{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C#: csBlazorLifeCycleEvent (http)",
      "type": "dotnet",
      "request": "launch",
      "projectPath": "${workspaceFolder}/csBlazorLifeCycleEvent/csBlazorLifeCycleEvent.csproj",
      "launchBrowser": {
        "enabled": true,
        "args": "${auto-detect-url}",
        "windows": {
          "command": "cmd.exe",
          "args": "/C start ${auto-detect-url}"
        }
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": "C#: csBlazorLifeCycleEvent (https)",
      "type": "dotnet",
      "request": "launch",
      "projectPath": "${workspaceFolder}/csBlazorLifeCycleEvent/csBlazorLifeCycleEvent.csproj",
      "launchBrowser": {
        "enabled": true,
        "args": "${auto-detect-url}",
        "windows": {
          "command": "cmd.exe",
          "args": "/C start ${auto-detect-url}"
        }
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:7019;http://localhost:5121"
      }
    }
  ]
}
